        "node.js",
        "es5",
        "javascript"
    ],
    "license": "MIT",
    "main": "./dist/lamb.js",
    "module": "./dist/lamb.mjs",
    "exports": {
        ".": {
            "require": "./dist/lamb.js",
            "import": "./dist/lamb.mjs",
            "default": "./dist/lamb.js"
        },
        "./package.json": "./package.json"
    },
    "name": "lamb",
    "repository": {
        "type": "git",
        "url": "https://github.com/ascartabelli/lamb.git"
    },
    "scripts": {
        "build": "gulp build",
        "coverage": "gulp test:coverage",
        "lint": "gulp lint",
        "test": "gulp test"
    },
    "babel": {
        "env": {
            "test": {
                "presets": [
                    [
                        "@babel/preset-env",
                        {
                            "targets": {
                                "node": "current"
                            }
                        }
                    ]
                ]
            }
        }
    },
    "jsdelivr": "dist/lamb.min.js",
